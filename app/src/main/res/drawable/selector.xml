<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        
        <shape>
            <solid android:color="#FFdd00" />

            <stroke android:width="1dip" android:color="#FFFFE0" />
        </shape>
    </item>
        
    <item android:drawable="@drawable/focusthree" android:state_window_focused="false"/>
    <item android:drawable="@drawable/focustwo" android:state_selected="true"/>
    <item android:drawable="@drawable/focustwo" android:state_focused="true"/>

</selector>